    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "sequelize": "^6.37.3",
    "sequelize-cli": "^6.6.2",
    "socket.io": "^4.7.5",
    "swagger-ui-express": "^5.0.0",
    "twilio": "^5.1.1",
    "typeorm": "^0.3.20"
